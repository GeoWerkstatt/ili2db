INTERLIS 2.3;

!!
!! assoc with stable ID based on referenced objects
MODEL AssocUpdate1
  AT "mailto:ce@eisenhutinformatik.ch" VERSION "2019-11-22" =
    
  TOPIC TestA =
    OID AS INTERLIS.UUIDOID;
  
    CLASS ClassA1 =
    	attrA : TEXT*20;
    END ClassA1;

    CLASS ClassB1=
    	attrB : TEXT*20;
    END ClassB1;

    ASSOCIATION a2b =
      a (EXTERNAL) -- {0..*} ClassA1;
      b (EXTERNAL) -- {0..*} ClassB1;
      attrAB : TEXT*20;
    END a2b;
    
  END TestA;
  
  
END AssocUpdate1.
